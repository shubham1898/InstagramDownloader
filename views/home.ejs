            <iframe src="/inframe/bar" frameborder="0" id=frame class=scroll ></iframe>
        </div>
        <!-- /////// -->

        <!-- footer note -->
        <!-- fixed-bottom -->
        <div class=" ml-5">
            <ol>
                <p class="mb-0">Notes :</p>
                <li> V2.0 Now access and Download all the media content of an Instagram user</li>
